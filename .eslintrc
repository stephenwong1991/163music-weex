{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "weex": true
  },
  "extends": [
    "plugin:vue/essential", 
    "standard"
  ],
  "plugins": [
    "vue"
  ],
  "rules": {
    "vue/no-parsing-error":  [2, {
      "x-invalid-end-tag": false
    }],
    "generator-star-spacing": "off",
    "vue/html-self-closing": "off"
  }
}
